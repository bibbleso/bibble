{
  "name": "xrpl.fyi",
  "version": "0.0.1-beta",
  "description": "This is an open-source registry of networks and side-chains within the XRPL ecosystem",
  "repository": "https://github.com/bibbleso/bibble.git",
  "author": "interc0der <intercoder@whirled.io>",
  "license": "MIT",
  "private": false,
  "main": "dist/src/index.js",
  "unpkg": "dist/src/index.js",
  "jsdelivr": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "c": "rm -rf ./dist && yarn format && yarn gen:docs && tsc",
    "gen:json": "yarn ts-node ./scripts/index.ts",
    "gen:types": "yarn ts-node ./scripts/genType.ts",
    "gen:docs": "npx typedoc --options typedoc.json",
    "format": "npx prettier -w .",
    "/prepublish": "yarn ts-node ./scripts/index.ts && yarn c",
    "test": "yarn c && jest -all"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.0.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.7.4",
    "yalc": "^1.0.0-pre.53"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "ts-loader": "^9.3.1"
  }
}
